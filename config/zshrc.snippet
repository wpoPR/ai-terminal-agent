# Add ~/bin to PATH if not already present
if [[ ":$PATH:" != *":$HOME/bin:"* ]]; then
  export PATH="$HOME/bin:$PATH"
fi

# Shell completion for ai command
if [[ -f "$HOME/.ai-workspace/ai-completion.zsh" ]]; then
  source "$HOME/.ai-workspace/ai-completion.zsh"
fi
